<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>SimpleServlet</display-name>
 	<servlet>
 	<servlet-name>SimpleServlet_one</servlet-name>
 	<servlet-class>org.mohan.SimpleServlet2</servlet-class>  
 	</servlet>
 	<servlet>
 	<servlet-name>SimpleServlet_two</servlet-name>
 	<servlet-class>org.mohan.SimpleServlet</servlet-class>  
 	</servlet>
 	<servlet-mapping>
 	<servlet-name>SimpleServlet_one</servlet-name>
 	<url-pattern>/SimpleServletPath2</url-pattern>
 	</servlet-mapping>
 	<servlet-mapping>
 	<servlet-name>SimpleServlet_two</servlet-name>
